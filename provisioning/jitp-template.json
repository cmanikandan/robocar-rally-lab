{
  "templateBody": {
    "Parameters" : {
      "AWS::IoT::Certificate::CommonName": {
        "Type": "String"
      },
      "AWS::IoT::Certificate::Id": {
        "Type": "String"
      }
    },
    "Resources" : {
      "thing" : {
        "Type" : "AWS::IoT::Thing",
        "Properties" : {
          "ThingName" : { "Ref" : "AWS::IoT::Certificate::CommonName" },
          "ThingTypeName": "DonkeyCar",
          "ThingGroups": [ "Robocars" ]
        }
      },
      "certificate" : {
        "Type" : "AWS::IoT::Certificate",
        "Properties" : {
          "CertificatePem": { "Ref" : "AWS::IoT::Certificate::Id" },
          "Status" : "ACTIVE"
        }
      },
      "policy" : {
        "Type" : "AWS::IoT::Policy",
        "Properties" : {
          "PolicyName" : "IotDonkeyCarPolicy"
        }
      }
    }
  },
  "roleArn": "arn:aws:iam::648414911232:role/IotRegistrationRole"
}